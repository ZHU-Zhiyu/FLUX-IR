{
    "name": "lolv1_test",
    "phase": "test",
    "gpu_ids": [
        0
    ],
    "seed": 733,
    "path": {
        "log": "logs",
        "tb_logger": "tb_logger",
        "results": "results",
        "checkpoint": "checkpoint", 
        // "resume_state": "./checkpoints/lolv1/lolv1_rein.pth"
        // "resume_state": "./checkpoints/lolv1/lolv1_dist2step.pth"
        "resume_state": "./checkpoints/lolv1/lolv1_dist1step.pth"

    },
    "model": {
        "which_model_G": "ddpm",
        "finetune_norm": false,
        "unet": {
            "in_channel": 6,
            "out_channel": 3,
            "inner_channel": 64,
            "channel_multiplier": [
                1,
                1,
                2,
                2,
                4
            ],
            "attn_res": [
                16
            ],
            "res_blocks": 2,
            "dropout": 0,
            "rm_varconv": false
        },
        "beta_schedule": {
            "train": {
                "schedule": "linear",
                "n_timestep": 10,
                "linear_start": 1e-4,
                "linear_end": 9.9e-1
            },
            "val": {
                "schedule": "linear",
                "n_timestep": 10,
                "linear_start": 2e-4,
                "linear_end": 9.3e-1
            }
        },
        "diffusion": {
            "image_size": 128,
            "channels": 6, 
            "conditional": true
        }
    },
    "train": {
        "n_iter": 1000000,
        "val_freq": 1e4,
        "save_checkpoint_freq": 5e4,
        "print_freq": 200,
        "optimizer": {
            "type": "adam",
            "lr": 1e-4
        },
        "ema_scheduler": {
            "step_start_ema": 5000,
            "update_ema_every": 1,
            "ema_decay": 0.9999
        }
    },
    "wandb": {
        "project": "llie_ddpm"
    }
}
